{
  "name": "autolease-ninja",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.153",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^6.0.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-slick": "^0.23.4",
    "@types/reactstrap": "^8.4.2",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "lodash": "^4.17.15",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-input-range": "^1.3.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-slick": "^0.26.1",
    "reactstrap": "^8.4.1",
    "slick-carousel": "^1.8.1",
    "tailwindcss": "^1.4.6",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "bundle": "webpack",
    "clean": "shx rm -rf ./dist",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open --hot",
    "dev": "npm-run-all --parallel start-dev-server start",
    "start-dev-server": "node ../autolease-ninja-middleware/app.js",
    "prod": "cross-env NODE_ENV=production webpack-dev-server --mode production --open --hot",
    "widget": "cross-env NODE_ENV=development webpack-dev-server --mode development --open --hot --entryDev=widget",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production",
    "invalidation-dev": "aws cloudfront create-invalidation --distribution-id E2RBIEG6H44NOM --path /*",
    "upload-dev": "aws s3 sync ./dist s3://bootsketch --cache-control max-age=604800",
    "deploy-dev": "npm run build && npm run upload-dev && npm run invalidation-dev",
    "upload": "aws s3 sync ./dist s3://autolease-frontend-react --cache-control max-age=604800 --profile autolease",
    "invalidation": "aws cloudfront create-invalidation --distribution-id E2YK1PKW8BAXGZ --path /* --profile autolease",
    "deploy": "npm run build && npm run upload && npm run invalidation"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-loader": "^3.0.0",
    "shx": "^0.3.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
